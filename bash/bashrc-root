# Sexy Bash Prompt
GREEN="\e[32m"
RED="\e[91m"
CYAN="\e[36m"
RESET="\033[m"

PS1="\[$CYAN\]\W\[$RED\]#\[$GREEN\]=> \[$RESET\]"

alias p="ping 8.8.8.8"
alias v="vim"
alias e="emacsclient"
alias ee="emacs"
alias s="subl"
alias t="tmux"
alias pdf="evince"
alias grep="grep -n --color"
alias rm="rm -i"
export EDITOR="vim"
